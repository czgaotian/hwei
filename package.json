{
  "name": "hwei",
  "description": "A CMS based React application with Vite, Hono, and Cloudflare Workers",
  "dependencies": {
    "@ant-design/icons": "6.x",
    "@hono/zod-openapi": "^1.2.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@tiptap/pm": "^3.16.0",
    "@tiptap/react": "^3.16.0",
    "@tiptap/starter-kit": "^3.16.0",
    "antd": "^6.1.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.45.1",
    "hono": "4.11.1",
    "jose": "^6.1.3",
    "lucia": "^3.2.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-router": "^7.11.0",
    "swr": "^2.3.8",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.15.3",
    "@eslint/js": "9.39.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.8",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.48.0",
    "vite": "^6.0.0",
    "wrangler": "4.54.0"
  },
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "db:generate": "drizzle-kit generate",
    "db:upgrade": "drizzle-kit up",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "wrangler d1 migrations apply blog-database --local",
    "db:migrate:prod": "wrangler d1 migrations apply blog-database --remote"
  },
  "type": "module"
}
